@model List<eadfinal.Models.movy>


@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Cancel Schedule </h1>



<div class="box-body table-responsive no-padding">
    <table class="table table-hover">
        <tr>
            <th>NAME</th>
            <th>Time</th>
            <th>Date</th>
            
            <th>Price</th>
        </tr>

        @foreach (var q in Model)
        {


            <tr>
                <td>@q.movieName</td>
                <td>@q.time</td>
                <td>@q.date</td>
                <td>@q.fare</td>
       
                <td><a href="/schedule/onDelete/@q.Id">Cancel</a></td>
            </tr>



        }
