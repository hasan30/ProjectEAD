@model List<eadfinal.Models.history>
@{
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<h1>view History </h1>



<div class="box-body table-responsive no-padding">
    <table class="table table-hover">
        <tr>
            <th>Movie Name</th>
            <th>Date</th>
            <th>Time</th>

            <th>Quantity</th>
            <th>Payment</th>
        </tr>

        @foreach (var q in Model)
        {


            <tr>
                <td>@q.m.movieName</td>
                <td>@q.m.date</td>
                <td>@q.m.time</td>

                <td>@q.t.quantity</td>
                <td>@q.t.payment</td>

            </tr>



        }
