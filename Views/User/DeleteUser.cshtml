@model List<eadfinal.Models.user>


@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Delete user </h1>



<div class="box-body table-responsive no-padding">
    <table class="table table-hover">
        <tr>
            <th>UserId</th>
            <th>Name</th>
            <th>Password</th>

            <th>Age</th>
            <th>Email</th>
        </tr>

        @foreach (var q in Model)
        {


            <tr>
                <td>@q.userName</td>
                <td>@q.name</td>
                <td>@q.password</td>

                <td>@q.age</td>
                <td>@q.email</td>
                <td> <a href="/User/onDelete/@q.Id">Delete</a></td>
                
            </tr>



        }
