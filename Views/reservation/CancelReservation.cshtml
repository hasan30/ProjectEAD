@model List<eadfinal.Models.ticket>


@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Cancel Reservation </h1>



<div class="box-body table-responsive no-padding">
    <table class="table table-hover">
        <tr>
            <th>TicketNo</th>
            <th>Name</th>
            <th>EMAIL</th>
            <th>Quantity</th>
            <th>Payment</th>
            <th>Cancel</th>
        </tr>

        @foreach (var q in Model)
        {


            <tr>
                <td>@q.Id</td>
                <td>@q.custumerName</td>
                <td>@q.email</td>

                <td>@q.quantity</td>
                <td>@q.payment</td>
                <td> <a href="/Reservation/onDelete/@q.Id">Delete</a></td>

            </tr>



        }
